// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Empresa {
  id         Int      @id @default(autoincrement())
  nome       String
  email      String
  telefone   String
  endereco   String
  is_ativo   Boolean
  created_at DateTime @default(now())

  @@map("empresas")
}
